{
  "compilerOptions": {
    "strict": true,
    "erasableSyntaxOnly": true,
    "noEmit": true,
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": false,
    "noUncheckedIndexedAccess": true
  }
}
